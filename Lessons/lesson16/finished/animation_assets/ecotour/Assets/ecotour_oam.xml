<?xml version="1.0" encoding="utf-8" ?>
<widget xmlns="http://openajax.org/metadata" spec="1.0" name="ecotour" version="1">

<description type="text/html"><![CDATA[Widget Exported from Flash Professional]]></description>

<category name="Flash Animation"/>
<remarks>Compatible with: InDesign CC, Dreamweaver CC, Muse</remarks>
<icon src="PosterImg_400.png" width="480" height="200"/>
<require type="other" src="ecotour.html"/>
<require type="javascript" src="ecotour.js"/>
<require type="image" src="images/ecotour_atlas_.png"/>
<require type="folder" src="images"/>
<content></content>

<properties name="Properties">
<property name="default-width" defaultValue="480" hidden="true"/>
<property name="default-height" defaultValue="200" hidden="true"/>
<property name="resizable" defaultValue="true" hidden="true"/>
<property name="centering" defaultValue="" hidden="true"/>
<property name="scaling" defaultValue="both" hidden="true"/>
<property name="html-page" defaultValue="ecotour.html" hidden="true"/>
<property name="target-platform" defaultValue="htmlcanvas" hidden="true"/>
<property name="created-in" defaultValue="Adobe Animate CC" hidden="true"/>
</properties>

</widget>
